<!DOCTYPE html>
<html lang="cs">
<head>
    <meta charset="utf-8">
    <meta name="description" content="">

    <title>{_admin.common.title, [seminar_name => $seminarName]}</title>

    <link rel="shortcut icon" href="{$basePath}/favicon.ico">

    {control css}
    {control js}

    {block head}{/block}
</head>

<body>
    <div id="main-menu">
        {import 'includes/main_menu.latte'}
        {include main-menu}
    </div>

    <div class="container-fluid" {$containerAttributes|noescape}>
        <div id="content" class="row">
            <div id="main" class="col">
                {snippet flashes}
                    <div id="flashes" class="row">
                        <div class="col-12">
                            <div n:foreach="$flashes as $flash" n:class="alert, alert-dismissable, 'alert-' . $flash->type">
                                {$flash->message|noescape}
                            </div>
                        </div>
                    </div>
                {/snippet}

                {include #main}
            </div>
        </div>

        <hr>

        <footer>
            <p>{$footer|noescape}</p>
        </footer>
    </div>
</body>
</html>




